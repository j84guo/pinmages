GET             /                   controllers.PgController.index
GET             /users/:name        controllers.PgController.getUser(name: String)
GET             /users              controllers.PgController.getAllUsers()
GET             /images             controllers.PgController.getAllImages()

POST            /users/:name        controllers.PgController.createUser(name: String)

PUT             /images/:name       controllers.PgController.createImage(name: String)

DELETE          /users/:name        controllers.PgController.deleteUser(name: String)